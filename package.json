{
  "name": "enterprise-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "postinstall": "prisma generate",
    "preinstall": "npx only-allow pnpm",
    "dev:app": "next dev --turbo --port 3030",
    "dev:trigger": "bun trigger-cli dev -p 3030",
    "dev": "concurrently --kill-others npm:dev:*",
    "build": "pnpm build:app && pnpm build:doc",
    "build:app": "next build",
    "build:doc": "typedoc",
    "knip": "knip",
    "test": "vitest",
    "start": "next start",
    "clean": "bun format; bun lint --fix --quiet; bun typecheck",
    "add:component": "bun shadcn-ui add",
    "lint:package_json": "npmPkgJsonLint .",
    "docker:build": "IS_DOCKER=true docker build -t t3-stack . --dockerfile=docker/prod.Dockerfile",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:eslint": "eslint --cache --ext .ts,.tsx --fix .",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "typecheck": "tsc --noEmit",
    "oxlint:lint": "oxlint",
    "biome:lint": "biome lint .",
    "biome:check": "biome check .",
    "biome:format": "biome format . --write",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "analyze": "cross-env ANALYZE=true pnpm build",
    "storybook": "cross-env FORCE_COLOR=1 storybook dev -p 6006",
    "test-storybook": "cross-env FORCE_COLOR=1 test-storybook",
    "build-storybook": "cross-env FORCE_COLOR=1 storybook build",
    "e2e:headless": "playwright test",
    "e2e:ui": "playwright test --ui",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "build:manifest": "pnpm tsx src/app/manifest.ts",
    "cf:pages:build": "pnpx @cloudflare/next-on-pages",
    "cf:preview": "pnpm run cf:pages:build && wrangler pages dev .vercel/output/static",
    "cf:deploy": "pnpm run cf:pages:build && wrangler pages deploy .vercel/output/static",
    "coupling-graph": "pnpm madge --extensions js,jsx,ts,tsx,css,md,mdx ./ --exclude '.next|tailwind.config.js|reset.d.ts|prettier.config.js|postcss.config.js|playwright.config.ts|next.config.js|next-env.d.ts|instrumentation.ts|e2e/|README.md|.storybook/|.eslintrc.js' --image graph.svg"
  },
