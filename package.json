{
  "name": "enterprise-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "postinstall": "prisma generate",
    "postbuild": "bash ./scripts/postbuild.sh",
    "dev:iframe": "pnpm build:iframe --watch",
    "dev:app": "next dev --turbo --port 3030",
    "dev:trigger": "sleep 3; pnpm trigger-cli dev -p 3030",
    "dev:stripe": "stripe listen --forward-to localhost:3030/api/trigger --skip-verify",
    "dev": "concurrently --kill-others npm:dev:*",
    "build": "NODE_OPTIONS=--max-old-space-size=6144 pnpm build:app && pnpm build:iframe && pnpm build:doc",
    "build:iframe": "bun scripts/build-public-iframe.ts",
    "build:app": "next build",
    "build:doc": "typedoc",
    "knip": "knip",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "start": "next start",
    "add:component": "pnpm shadcn-ui add",
    "docker:build": "IS_DOCKER=true docker build -t pulse-app . --dockerfile=docker/prod.Dockerfile",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "typecheck": "tsc --noEmit",
    "lint:package": "npmPkgJsonLint .",
    "lint:next": "next lint --fix --quiet",
    "format": "pnpm lint:oxlint; pnpm lint:biome",
    "lint:biome": "biome check --unsafe --write .",
    "lint:eslint": "eslint --quiet --cache --fix .",
    "lint": "pnpm lint:oxlint; pnpm lint:biome; pnpm lint:eslint",
    "lint:oxlint": "oxlint -c .oxlintrc.json --fix --import-plugin --jest-plugin --jsx-a11y-plugin --nextjs-plugin --react-perf-plugin --ignore-path .oxlintignore",
    "check": "pnpm typecheck; pnpm lint:package; pnpm lint:oxlint; pnpm lint:biome; pnpm lint:eslint",
    "analyze": "cross-env ANALYZE=true pnpm build",
    "storybook": "cross-env FORCE_COLOR=1 storybook dev -p 6006",
    "test-storybook": "cross-env FORCE_COLOR=1 storybook test",
    "build-storybook": "cross-env FORCE_COLOR=1 storybook build",
    "e2e:headless": "playwright test",
    "e2e:ui": "playwright test --ui",
    "build:manifest": "pnpm tsx src/app/manifest.ts",
    "tune": "pnpm tsx scripts/fine-tune/index.ts",
    "coupling-graph": "pnpm madge --extensions js,jsx,ts,tsx,css,md,mdx ./ --exclude '.next|tailwind.config.js|reset.d.ts|postcss.config.js|playwright.config.ts|next.config.js|next-env.d.ts|instrumentation.ts|e2e/|README.md|.storybook/|.eslintrc.js' --image graph.svg",
    "cf:pages:build": "pnpx @cloudflare/next-on-pages",
    "cf:preview": "pnpm run cf:pages:build && wrangler pages dev .vercel/output/static",
    "cf:deploy": "pnpm run cf:pages:build && wrangler pages deploy .vercel/output/static",
    "setup:stripe": "brew install stripe/stripe-cli/stripe; stripe login"
  },
