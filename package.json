{
  "name": "enterprise-starter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "start": "next start",
    "postinstall": "prisma generate",
    "postbuild": "bash ./scripts/postbuild.sh",
    "dev:iframe": "pnpm build:iframe --watch",
    "dev:app": "next dev --turbo --port 3030",
    "dev:trigger": "pnpx trigger.dev@beta dev || true",
    "dev:stripe": "which stripe >/dev/null 2>&1 && stripe listen --forward-to localhost:3000/api/trigger --skip-verify || echo 'Stripe not available'",
    "dev": "concurrently --kill-others-on-fail npm:dev:*",
    "build": "NODE_OPTIONS=--max-old-space-size=8192 pnpm build:app && pnpm build:iframe && pnpm build:doc",
    "build:iframe": "pnpm bun scripts/build-public-iframe.ts",
    "build:app": "next build",
    "build:doc": "typedoc",
    "knip": "knip",
    "test:unit": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "add:component": "pnpx shadcn@latest add",
    "docker:build": "IS_DOCKER=true docker build -t enterprise-starter . --dockerfile=docker/prod.Dockerfile",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "lint:package": "npmPkgJsonLint .",
    "lint:next": "next lint --fix --quiet",
    "format": "pnpm lint:oxlint; pnpm lint:biome",
    "lint:biome": "biome check --unsafe --write .",
    "lint": "pnpm lint:oxlint; pnpm lint:biome; pnpm lint:eslint",
    "typecheck": "NODE_OPTIONS='--max-old-space-size=8192' tsc --noEmit",
    "lint:eslint": "NODE_OPTIONS='--max-old-space-size=8192' eslint --quiet --cache --fix",
    "lint:oxlint": "oxlint -c oxlintrc.json --fix --import-plugin --jest-plugin --jsx-a11y-plugin --nextjs-plugin --react-perf-plugin --ignore-path .oxlintignore -A label-has-associated-control",
    "check": "pnpm typecheck; pnpm lint:package; pnpm lint:oxlint; pnpm lint:biome; pnpm lint:eslint",
    "storybook": "cross-env FORCE_COLOR=1 storybook dev -p 6006",
    "test:storybook": "cross-env FORCE_COLOR=1 test-storybook",
    "build:storybook": "cross-env FORCE_COLOR=1 storybook build",
    "lint:circular": "pnpm madge --circular --extensions ts,tsx,mjs,js ./",
    "build:manifest": "pnpm tsx src/app/manifest.ts",
    "tune": "pnpm tsx scripts/fine-tune/index.ts",
    "analyze": "cross-env ANALYZE=true pnpm build",
    "coupling-graph": "pnpm madge --extensions js,jsx,ts,tsx,css,md,mdx ./ --exclude '.next|tailwind.config.js|reset.d.ts|postcss.config.js|playwright.config.ts|next.config.js|next-env.d.ts|instrumentation.ts|e2e/|README.md|.storybook/|eslint.config.mjs' --image graph.svg",
    "cf:pages:build": "pnpx @cloudflare/next-on-pages",
    "cf:preview": "pnpm run cf:pages:build && wrangler pages dev .vercel/output/static",
    "cf:deploy": "pnpm run cf:pages:build && wrangler pages deploy .vercel/output/static",
    "setup:stripe": "brew install stripe/stripe-cli/stripe; stripe login",
    "prereqs:install": "brew install vips pkg-config gcc gnu-sed coreutils gnupg"
  },
